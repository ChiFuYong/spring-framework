<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <!-- 开启注解扫描 -->
    <context:component-scan base-package="cn.chifuyong"></context:component-scan>

	<!--测试 BeanDefinitionRegisterPostProcessor-->
	<bean class="cn.chifuyong.postprocessors.BeanDefinitionRegisterPostProcessorTest"></bean>

	<!--测试 BeanFactoryPostProcessor，执行顺序按从上往下顺序-->
	<bean class="cn.chifuyong.postprocessors.BeanFactoryPostProcessorTest2"></bean>
	<bean class="cn.chifuyong.postprocessors.BeanFactoryPostProcessorTest1"></bean>

	<!--测试 FactoryBean-->
	<bean id="env" class="cn.chifuyong.factorybean.FactoryBeanTest"></bean>

</beans>